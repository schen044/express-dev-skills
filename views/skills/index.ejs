<%- include('../partials/header') %>
    <h1>Skills</h1>
    <ul>
        <% skills.forEach(function(skill) { %>
          <li>
            <a href="/skills/<%= skill.id %>">
                <%= skill.skillName %>
            </a>
             used in 
            <%= skill.project %>
          </li>
        <% }); %>
      </ul>
      <a href="/skills/new">Add a skill</a>
</body>
</html>